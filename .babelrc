{
  "env": {
    "production": {
      "presets": [
        [
          "@babel/preset-env", {
            "loose": true,
            "modules": false,
            "useBuiltIns": "usage",
            "targets": {
              "browsers": "defaults"
            }
          }
        ]
      ],
      "plugins": [
        "@babel/plugin-proposal-do-expressions",
        "@babel/plugin-proposal-export-default-from",
        "@babel/plugin-proposal-nullish-coalescing-operator",
        "@babel/plugin-proposal-optional-chaining",
        "@babel/plugin-proposal-pipeline-operator"
      ]
    },
    "commonjs": {
      "presets": [
        [
          "@babel/preset-env", {
            "loose": true,
            "useBuiltIns": "usage",
            "targets": {
              "browsers": "defaults"
            },
            "modules": "commonjs"
          }
        ]
      ],
      "plugins": [
        "@babel/plugin-proposal-do-expressions",
        "@babel/plugin-proposal-export-default-from",
        "@babel/plugin-proposal-nullish-coalescing-operator",
        "@babel/plugin-proposal-optional-chaining"
      ]
    },
    "es": {
      "presets": [
        [
          "@babel/preset-env", {
            "debug": true,
            "modules": false,
            "useBuiltIns": "usage"
          }
        ]
      ],
      "plugins": [
        "@babel/plugin-proposal-do-expressions",
        "@babel/plugin-proposal-export-default-from",
        "@babel/plugin-proposal-nullish-coalescing-operator",
        "@babel/plugin-proposal-optional-chaining"
      ]
    },
    "development": {
      "presets": [
        [
          "@babel/preset-env", {
            "loose": true,
            "modules": false,
            "useBuiltIns": "usage",
            "targets": {
              "browsers": ["chrome >= 56"]
            }
          }
        ]
      ],
      "plugins": [
        "@babel/plugin-proposal-do-expressions",
        "@babel/plugin-proposal-export-default-from",
        "@babel/plugin-proposal-nullish-coalescing-operator",
        "@babel/plugin-proposal-optional-chaining"
      ]
    },
    "cli": {
      "presets": [
        [
          "@babel/preset-env", {
            "loose": true,
            "modules": false,
            "useBuiltIns": "usage"
          }
        ]
      ],
      "plugins": [
        "@babel/plugin-proposal-do-expressions",
        "@babel/plugin-proposal-export-default-from",
        "@babel/plugin-proposal-nullish-coalescing-operator",
        "@babel/plugin-proposal-optional-chaining"
      ]
    }
  }
}
